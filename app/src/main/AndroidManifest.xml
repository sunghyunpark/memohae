<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yssh.memohae">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_icon_144"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="view.SettingActivity" />
        <activity android:name="view.WriteMemoActivity" />
        <activity android:name="view.ColorSettingActivity" />
        <activity android:name="view.EditMemoActivity" />
        <activity android:name="view.HelpActivity" />
        <activity android:name="view.PatternActivity" />
        <activity android:name="view.ImageViewerActivity"></activity>
    </application>

</manifest>